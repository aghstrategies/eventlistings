<?xml version="1.0"?>
<extension key="eventlistings" type="module">
  <file>eventlistings</file>
  <name>Event Listings</name>
  <description>Creates an Upcoming Events list that can be embedded where ever you like.</description>
  <license>AGPL-3.0</license>
  <maintainer>
    <author>Alice Frumin</author>
    <email>alice@aghstrategies.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/aghstrategies/eventlistings/</url>
    <url desc="Documentation">https://github.com/aghstrategies/eventlistings/</url>
    <url desc="Support">https://github.com/aghstrategies/eventlistings/</url>
    <url desc="Licensing">http://www.gnu.org/licenses/agpl-3.0.html</url>
  </urls>
  <releaseDate>2025-12-30</releaseDate>
  <version>1.0</version>
  <develStage>alpha</develStage>
  <compatibility>
    <ver>6.9</ver>
  </compatibility>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <civix>
    <namespace>CRM/Eventlistings</namespace>
    <format>23.02.1</format>
    <angularModule>crmEventlistings</angularModule>
  </civix>
  <mixins>
    <mixin>mgd-php@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
</extension>
